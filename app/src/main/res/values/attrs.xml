<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyRadioButton">
        <attr name="drawableSizes" format="dimension"/>
        <attr name="drawableTop" format="reference"/>
        <attr name="drawableLeft" format="reference"/>
        <attr name="drawableRight" format="reference"/>
        <attr name="drawableBottom" format="reference"/>
    </declare-styleable>

    <declare-styleable name="AttendanceView">
        <attr name="beginTime" format="string"/>
        <attr name="endTime" format="string"/>
        <attr name="beginStateColor" format="color"/>
        <attr name="endStateColor" format="color"/>
        <attr name="textSize" format="dimension"/>
    </declare-styleable>

</resources>
